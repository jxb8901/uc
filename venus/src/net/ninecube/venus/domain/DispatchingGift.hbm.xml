<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="TDispatchingGift" name="net.ninecube.venus.domain.DispatchingGift">
    <meta inherit="true" attribute="auto-generated"/>
    <id access="property" name="idno">
      <generator class="native"/>
    </id>
    <property name="applyDate" access="property"/>
    <many-to-one access="property" name="customer"/>
    <many-to-one access="property" name="gift"/>
    <property name="quantity" access="property"/>
    <property name="status" access="property"/>
  </class>
</hibernate-mapping>
