<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="TRules" name="net.ninecube.venus.domain.RuleEntity">
    <meta inherit="true" attribute="auto-generated"/>
    <id access="property" name="idno">
      <generator class="native"/>
    </id>
    <property name="description" access="property"/>
    <property name="name" access="property"/>
    <property name="rule" lazy="true" type="org.springframework.orm.hibernate3.support.ClobStringType" access="property" column="frule"/>
    <property name="type" access="property"/>
    <property name="mapping" lazy="true" type="org.springframework.orm.hibernate3.support.ClobStringType" access="property"/>
  </class>
</hibernate-mapping>
