<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="TGift" name="net.ninecube.venus.domain.Gift">
    <meta inherit="true" attribute="auto-generated"/>
    <id access="property" name="idno">
      <generator class="native"/>
    </id>
    <property name="availableQuantity" access="property"/>
    <property name="costPrice" access="property"/>
    <property name="description" access="property"/>
    <property name="exchangeRule" access="property"/>
    <property name="marketPrice" access="property"/>
    <property name="name" access="property"/>
    <property name="seqno" access="property" unique="true"/>
    <property name="image" length="1116777216" type="org.springframework.orm.hibernate3.support.BlobByteArrayType" access="property"/>
    <property name="imageFileName" access="property"/>
  </class>
</hibernate-mapping>
