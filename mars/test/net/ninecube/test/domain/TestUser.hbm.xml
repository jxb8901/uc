<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="TestUser" name="net.ninecube.test.domain.TestUser">
    <id length="80" access="property" name="idno">
      <generator class="native"/>
    </id>
    <property name="userid" access="property"/>
    <property name="amount" access="property"/>
    <property name="creationDate" access="property"/>
    <property name="description" access="property"/>
    <property name="name" access="property"/>
    <property name="password" access="property"/>
  </class>
</hibernate-mapping>
